<!-- partials/seo -->
<meta name="description" content="<%= page.description || app.tagline %>" />
<meta property="og:title" content="<%= page.title || app.tagline %>" />
<meta property="og:description" content="<%= page.description || app.tagline %>" />
<meta property="og:image" content="<%= app.homeUrl + page.image %>" />
<meta property="og:url" content="<%= app.homeUrl + page.slug %>" />
<meta property="og:type" content="website" />
<meta property="og:site_name" content="<%= app.name %>" />
<meta name="twitter:card" content="summary_large_image" />
<meta name="twitter:title" content="<%= page.title || app.tagline %>" />
<meta name="twitter:description" content="<%= page.description || app.tagline %>" />
<meta name="twitter:image" content="<%= app.homeUrl + page.image %>" />
<!-- /partials/seo -->